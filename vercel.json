{
    "projects": [
      {
        "name": "web",
        "path": "web",
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      },
      {
        "name": "sanity",
        "path": "sanity",
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "web/$1" },
      { "src": "/studio/(.*)", "dest": "sanity/$1" }
    ],
    "domains": [
      {
        "domain": "codetricks.xyz",
        "default": true
      },
      {
        "domain": "studio.codetricks.xyz",
        "path": "sanity"
      }
    ]
  }
  